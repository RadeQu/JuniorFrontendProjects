ox{\r\n            @include flex (start, center, column);\r\n            background-color: $Soft_red;\r\n            width: 100%;\r\n            border-radius: 15px;\r\n            padding: 3%;\r\n            position: relative;\r\n            margin-bottom: 3%;\r\n            .myBalanceTitle{\r\n               font-size: 16px;\r\n               color: $Very_pale_orange;\r\n               font-weight: 400; \r\n               margin-bottom: 2%;\r\n              \r\n            }\r\n            .myMoney{\r\n                color: $Very_pale_orange;\r\n                font-weight: 700; \r\n                font-size: 24px;\r\n            }\r\n            img{\r\n                position: absolute;\r\n                align-self: flex-end;\r\n                justify-self: center;\r\n            }\r\n        }\r\n        .content{\r\n            @include flex (start, start, column);\r\n            background-color: $Very_pale_orange;\r\n            width: 100%;\r\n            border-radius: 15px;\r\n            padding: 4%;\r\n            height: 100%;\r\n            position: relative;\r\n            .title{\r\n                color: $Dark_brown;\r\n                margin-left: 2%;\r\n            }\r\n            .table{\r\n               position: absolute;\r\n               bottom: 25%;\r\n                width: 100%;\r\n                right: 0;\r\n                padding: 1%;\r\n                .bars{\r\n                    @include flex (flex-end, flex-end, row);\r\n                    height: 32vh;\r\n                    width: 100%;\r\n                    .bar{\r\n                        @include flex(null, null, row);\r\n                        padding: 5% 4% 0;\r\n                        margin: 0 3.1%;\r\n                        background-color: $Soft_red;\r\n                        border-radius: 5px; \r\n                        transition: all 0.3s;\r\n                        position: relative;\r\n                        opacity: 1;\r\n                        cursor: pointer;\r\n                    }\r\n                    .bar::after{\r\n                        content: attr(data-content);\r\n                        background-color: $Dark_brown;\r\n                        color: $Very_pale_orange;\r\n                        width: 120%;\r\n                        height: auto;                       \r\n                        border-radius: 5px;\r\n                        text-align: center;\r\n                        position: absolute;\r\n                        left: -10%;\r\n                        top: -35px;\r\n                    }\r\n                }\r\n                .days{\r\n                    @include flex (flex-end, flex-end, row);\r\n                    width: 100%;\r\n                    span{\r\n                        width: 100%;\r\n                        margin: 0 4.2%;\r\n                        color: $Medium_brown\r\n                    }\r\n                }\r\n            }\r\n            .lowerSection{\r\n                @include flex (center,space-around, row);\r\n                width: 92%;\r\n                margin-top: 5%;\r\n                padding: 5% 0;\r\n                border-top: 1px solid $Cream;\r\n                position: absolute;\r\n                bottom: 0;\r\n                .leftSide{\r\n                    margin-right: 30%;\r\n                    .titleUnder{\r\n                        color: $Medium_brown;\r\n                    }\r\n                    .priceMonth{\r\n                        font-size: 32px;\r\n                        color: $Dark_brown;\r\n                    }\r\n                }\r\n                .right{\r\n                    @include flex(null, null, column);\r\n                    span{\r\n                        color: $Medium_brown;\r\n                    }\r\n                    .percentage{\r\n                        font-weight: 700;\r\n                        text-align: end;\r\n                        color: $Dark_brown;\r\n                    }\r\n                }\r\n            }\r\n        }\r\n\t}\r\n\t.attribution {\r\n\t\tposition: absolute;\r\n\t\tbottom: 0;\r\n\t\twidth: 100%;\r\n\t\tfont-size: 11px;\r\n\t\ttext-align: center;\r\n\t\ta {\r\n\t\t\tcolor: hsl(228, 45%, 44%);\r\n\t\t}\r\n\t}\r\n}\r\n","scss\\abstracts\\_mixins.scss":"@mixin flex ($align, $justify, $dir) {\r\n    display: flex;\r\n    justify-content: $justify;\r\n    align-items: $align;\r\n    flex-direction: $dir;\r\n}\r\n@mixin mobile {\r\n    @media screen and(max-width:765px) {\r\n        @content;\r\n    }\r\n}","scss\\abstracts\\_variables.scss":"//Colors\r\n$Dark_brown: hsl(25, 47%, 15%);\r\n$Medium_brown: hsl(28, 10%, 53%);\r\n$Cream: hsl(27, 66%, 92%);\r\n$Very_pale_orange: hsl(33, 100%, 98%);\r\n$Soft_red: hsl(10, 79%, 65%);\r\n$Cyan: hsl(186, 34%, 60%);","scss\\breakpoints\\_mobile.scss":"@use \"../abstracts/index\" as *;\r\n\r\n@include mobile{\r\n    body{\r\n        overflow: hidden;\r\n        .wrapper{\r\n            width: 80%;\r\n            height: 85%;\r\n            overflow: hidden;\r\n            .balanceBox{\r\n                padding: 5%;\r\n            }\r\n            .content{\r\n                height: 50%;\r\n                .title{\r\n                    font-size: 22px;\r\n                }\r\n                .table{\r\n                    padding: 1%;\r\n                    .days{\r\n                        @include flex (center, flex-start, row);\r\n                        width: 60%;\r\n                        span{\r\n                           margin: 0 3.75%;\r\n                        }\r\n                    }\r\n                }\r\n                .lowerSection{\r\n                    width: 100%;\r\n                    margin-top: 10%;\r\n                    padding: 5% 0% 5%;\r\n                    .leftSide{\r\n                        margin-right: 10%;\r\n                        .titleUnder{\r\n                            font-size: 16px;\r\n                        }\r\n                    }\r\n                    .right{\r\n                        margin-right: 20%;\r\n                        span{\r\n                            font-size: 16px;\r\n                            width: 150%;\r\n                        }\r\n                        .percentage{\r\n                            font-size: 20px;\r\n                            padding-right: 15%;\r\n                        }\r\n                    }\r\n                }\r\n            }\r\n        }\r\n        .attribution {\r\n            display: none;\r\n        }\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"6559c50514fbbb9c3d1e7e77b8ff4063","cacheData":{"env":{}}}